{
  "routes": [
    {
      "src": "/index.js",
      "status": 308,
      "headers": {
        "Location": "https://rbc-backend.vercel.app/"
      }
    },
    {
      "src": "/api/users",
      "headers": { "cache-control": "s-maxage=1000" },
      "dest": "/index.js"
    },
    { "src": "/api/users", "dest": "./src/app.js" },
    { "src": "/api/users", "methods": ["get"], "dest": "./src/app.js" },
    { "src": "/users/(?<id>[^/]*)", "dest": "/users-api.js?id=$id" },
    { "src": "/legacy", "status": 404 },
    { "src": "/.*", "dest": "https://my-old-site.com" }
  ]
}
